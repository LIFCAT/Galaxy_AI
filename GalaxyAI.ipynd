            for (let i = 0; i < 80; i++) { // –ë–æ–ª—å—à–µ –∑–≤–µ–∑–¥
                const star = document.createElement("div");
                star.className = "star";
                star.style.width = `${2 * Math.random() + 1}px`;
                star.style.height = star.style.width;
                star.style.left = `${100 * Math.random()}%`;
                star.style.top = `${100 * Math.random()}%`;
                star.style.opacity = `${0.6 * Math.random() + 0.2}`;
                star.style.animationDelay = `${Math.random() * 4}s`;
                stars.appendChild(star);
            }
        }

        // üî• –£–õ–£–ß–®–ï–ù–ù–´–ï –ê–ù–ò–ú–ê–¶–ò–ò –ú–ï–ù–Æ
        function toggleMenu() {
            const menuPanel = document.getElementById("menuPanel");
            if (menuPanel.style.display === "block") {
                menuPanel.style.animation = "fadeIn 0.3s ease-out reverse";
                setTimeout(() => {
                    menuPanel.style.display = "none";
                }, 250);
            } else {
                menuPanel.style.display = "block";
                menuPanel.style.animation = "fadeIn 0.3s ease-out";
            }
            hideSubmenus();
        }

        function toggleSubmenu(type) {
            const submenu = document.getElementById(type + "Submenu");
            if (submenu.style.display === "block") {
                submenu.style.animation = "fadeIn 0.3s ease-out reverse";
                setTimeout(() => {
                    submenu.style.display = "none";
                }, 250);
            } else {
                submenu.style.display = "block";
                submenu.style.animation = "fadeIn 0.3s ease-out";
            }
        }

        function hideSubmenus() {
            document.querySelectorAll(".submenu").forEach(submenu => {
                submenu.style.animation = "fadeIn 0.3s ease-out reverse";
                setTimeout(() => {
                    submenu.style.display = "none";
                }, 250);
            });
        }

